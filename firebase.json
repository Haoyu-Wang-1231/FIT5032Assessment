{
  "firestore": {
    "database": "(default)",
    "location": "australia-southeast2",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "location": "australia-southeast1",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "emulators": {
    "hub": {
      "host": "127.0.0.1",
      "port": 4400
    },
    "exportOnExit": "./.emulator-data",
    "ui": {
      "enabled": true,
      "host": "127.0.0.1",
      "port": 4000
    },
    "auth": {
      "host": "127.0.0.1",
      "port": 9099
    },
    "firestore": {
      "host": "127.0.0.1",
      "port": 8080
    },
    "functions": {
      "host": "127.0.0.1",
      "port": 5001
    }
  }
}